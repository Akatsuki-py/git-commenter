{
  "name": "git-commenter",
  "version": "1.2.1",
  "private": true,
  "description": "Git Comment Generator with GUI",
  "main": "src/electron-starter.js",
  "author": "Akihiro Otomo🍁",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "^16.8.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1"
  },
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "build-windows-package": "npx electron-packager . GitCommenter --platform=win32 --arch=x64 --icon=./icon.ico --overwrite",
    "build-osx-package": "npx electron-packager . GitCommenter --platform=darwin --arch=x64 --icon=./icon.ico --overwrite",
    "build-linux-package": "npx electron-packager . GitCommenter --platform=linux --arch=x64 --icon=./icon.ico --overwrite"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-packager": "^13.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Akatsuki-py/git-commenter"
  }
}
